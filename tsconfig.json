{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "src",
    "paths": {
      "@/*": ["./*"],
      "@components/*": ["components/*"],
      "@assets/*": ["assets/*"],
      "@hooks/*": ["hooks/*"],
      "@screens/*": ["screens/*"],
      "@utils/*": ["utils/*"],
      "@navigation/*": ["navigation/*"],
      "@context/*": ["context/*"],
      "@constants/*": ["constants/*"],
      "@styles/*": ["styles/*"],
      "@services/*": ["services/*"],
      "@store/*": ["store/*"],
      "@types/*": ["types/*"],
      "@mocks/*": ["mocks/*"],
      "@config/*": ["config/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
